<command>
    <name>validate federation token</name>
    <description>Validates a Federation token.</description>
    <type>Java Method</type>
    <class>FederatedSecurityComponents</class>
    <method>validateFederationToken</method>
    <argument name="token" required="yes" datatype="string" />
    <documentation>
        <remarks>
