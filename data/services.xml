<services>

  <service>
    <application>moca</application>
    <displayName>RedPrairie MOCA Server (%MOCA_ENVNAME%)</displayName>
    <description>The MOCA Server for %MOCA_ENVNAME%.</description>

    <start>
      <mode>java</mode>
      <class>com.sam.moca.server.MocaServerMain</class>
      <debug/>
    </start>

    <stop>
      <mode>java</mode>
      <class>com.sam.moca.server.MocaServiceFunctions</class>
      <argument>stop</argument>
    </stop>

  </service>

</services>

